{
  "name": "gh copy title and link",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Extension to add copy title and link button",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/pull/*"],
      "js": ["main.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  }
}
